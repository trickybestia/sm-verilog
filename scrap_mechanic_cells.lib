  cell(NAND2) {
    area: 4;
    pin(A) { direction: input; }
    pin(B) { direction: input; }
    pin(Y) { direction: output;
             function: "!(A*B)"; }
  }
  cell(NAND3) {
    area: 5;
    pin(A) { direction: input; }
    pin(B) { direction: input; }
    pin(C) { direction: input; }
    pin(Y) { direction: output;
             function: "!(A*B*C)"; }
  }
  cell(NAND4) {
    area: 6;
    pin(A) { direction: input; }
    pin(B) { direction: input; }
    pin(C) { direction: input; }
    pin(D) { direction: input; }
    pin(Y) { direction: output;
             function: "!(A*B*C*D)"; }
  }

  cell(NOR1) {
    area: 3;
    pin(A) { direction: input; }
    pin(Y) { direction: output;
              function: "!A"; }
  }
  cell(NOR2) {
    area: 4;
